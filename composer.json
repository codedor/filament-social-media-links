{
    "name": "codedor/filament-social-media-links",
    "description": "Package to manage social media links in Flament and display them in your front-end",
    "keywords": [
        "codedor",
        "laravel",
        "filament-social-media-links"
    ],
    "homepage": "https://github.com/codedor/filament-social-media-links",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "codedor/filament-media-library": "dev-feature/filament-v5",
        "codedor/filament-settings": "dev-feature/filament-v5",
        "codedor/laravel-locale-collection": "^1.2",
        "filament/forms": "^4.0|^5.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "larastan/larastan": "^2.0|^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "phpstan/extension-installer": "^1.1|^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.0|^2.0",
        "phpstan/phpstan-phpunit": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Codedor\\SocialMediaLinks\\": "src",
            "Codedor\\SocialMediaLinks\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codedor\\SocialMediaLinks\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedor\\SocialMediaLinks\\Providers\\SocialMediaLinksServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
